    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-config-dev": "^2.0.0",
    "phtml-tape": "^1.1.3",
    "pre-commit": "^1.2.2",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2"
  },
  "eslintConfig": {
    "extends": "dev",
    "parser": "babel-eslint"
  },
  "keywords": [
    "phtml",
